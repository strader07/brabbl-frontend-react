{
  "name": "brabbl",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "webpack-server": "webpack-dev-server --hot --progress --colors",
    "web-server": "http-server -p 3000 .",
    "lint": "eslint . --fix --ext .js --ext .jsx || true",
    "api-mock-server": "json-server -p 4000 mock-api/discussions.json",
    "build_production": "APP_ENV=production NODE_ENV=production webpack -p --config webpack.production.config.js",
    "build_staging": "APP_ENV=staging NODE_ENV=production webpack -p --config webpack.production.config.js",
    "deploy_production": "npm run build_production && rsync -rv dist/build/production/ brabbl@api.brabbl.com:frontend/",
    "deploy_staging": "npm run build_staging && rsync -rv dist/build/staging/ brabbl-staging@api.brabbl.com:frontend/",
    "start": "npm run webpack-server & npm run web-server",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ignore-styles --require ./test/setup.js --recursive",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "axios": "^0.7.0",
    "babel-runtime": "^5.8.38",
    "classlist-polyfill": "^1.0.3",
    "es6-promise": "^3.1.2",
    "i18next": "^3.3.1",
    "i18next-xhr-backend": "^0.6.0",
    "moment": "^2.11.2",
    "newforms": "^0.13.2",
    "react": "^0.14.7",
    "react-avatar": "^0.7.1",
    "react-avatar-editor": "^3.2.1",
    "react-crouton": "^0.2.7",
    "react-datetime": "2.4.0",
    "react-dom": "^0.14.7",
    "react-lazy-load": "^3.0.10",
    "react-modal": "^0.6.0",
    "react-redux": "^4.4.0",
    "react-slider": "^0.6.0",
    "react-tagsinput": "^3.6.0",
    "react-youtube": "7.2.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "tiny-cookie": "^0.5.5"
  },
  "devDependencies": {
    "babel-core": "^5.8.24",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-react": "^4.3.0",
    "file-loader": "^0.8.5",
    "http-server": "^0.9",
    "ignore-styles": "^1.2.0",
    "img-loader": "^1.2.0",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.3",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "nyc": "^7.0.0",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.2.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^0.1.6",
    "redbox-react": "1.2.6",
    "sass-loader": "^3.1.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "grunt": "1.0.1",
    "grunt-react": "^0.10.0"
  },
  "author": "Jonas und der Wolf GmbH"
